      <string id="ForceInstantWake_2">Tvingad av</string>
      <string id="ForceInstantLock_DisplayName">Framtvinga omedelbar låsning</string>
      <string id="ForceInstantLock_ExplainText">Anger om låset ska vara tvingande av/på enligt MDM-principen. Användaren kan inte ändra den här inställningen och växlingen i gränssnittet blir nedtonad.</string>
      <string id="ForceInstantLock_0">Standard för användarval</string>
      <string id="ForceInstantLock_1">Tvingad på</string>
      <string id="ForceInstantLock_2">Tvingad av</string>
      <string id="ForceLockTimeout_DisplayName">Tidsgräns för lås</string>
      <string id="ForceLockTimeout_ExplainText">Anger om tidsgränsen för låset ska vara tvingande enligt MDM-principen. Användaren kan inte ändra den här inställningen och växlingen i gränssnittet blir nedtonad.</string>
      <string id="ForceLockTimeout_0">Standard för användarval</string>
